@model finalProject.Models.User
@{ 
    if (Session["userId"] != null)
    {
        Response.Redirect(Url.Action("Index", "Worker"));
    }
}
@using (Html.BeginForm("AutorizeUser","Home", FormMethod.Post))
{
    <img class="mb-4" src="/docs/4.2/assets/brand/bootstrap-solid.svg" alt="" width="72" height="72">
    <h1 class="h3 mb-3 font-weight-normal">Please sign in</h1>
    <label for="inputEmail" class="sr-only">Email address</label>
    <input type="text" name="inputIdMain" id="inputIdMain" class="form-control" placeholder="UserId" required autofocus>
    <label for="inputPassword" class="sr-only">Password</label>
    <input type="password" name="inputPasswordMain" id="inputPasswordMain" class="form-control" placeholder="Password" required>
    <br />
    <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
    <br />
    <p style="color:red">@Model.logInErorMassege</p>
}
